{
  "name": "image-proxy",
  "version": "1.0.0",
  "description": "## Prerequisities",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "run-docker": "open --background -a Docker && while ! docker system info > /dev/null 2>&1; do sleep 1; done",
    "build-docker": "rm -rf node_modules/sharp && docker run -v \"$PWD\":/var/task lambci/lambda:build-nodejs6.10 npm install",
    "strip-stuff": "rm -rf node_modules/aws-sdk && rm -rf node_modules/filterous/bin && rm node_modules/canvas/build/Release/{libjpeg.so.62,libpixman-1.so.0,libfreetype.so.6,libcairo.so.2,libpango-1.0.so.0,libpangocairo-1.0.so.0,libpangoft2-1.0.so.0}",
    "zip": "zip -r ../image-proxy *",
    "build": "npm run run-docker && npm run build-docker && npm run strip-stuff && npm run zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/neap/image-proxy.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/neap/image-proxy/issues"
  },
  "homepage": "https://gitlab.com/neap/image-proxy#README",
  "dependencies": {
    "aws-sdk": "^2.243.1",
    "axios": "^0.18.0",
    "filterous": "git+https://github.com/bberak/filterous-2.git",
    "sharp": "^0.20.1"
  },
  "devDependencies": {
    "mocha": "^5.1.1"
  }
}
